<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netcoreapp2.0</TargetFramework>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<AssemblyName>NiTiSCore</AssemblyName>
		<AssemblyVersion>1.3.0.0</AssemblyVersion>
		<Authors>NickName73,NiTiS</Authors>
		<Description>Core for NiTiS projects</Description>
		<Version>1.3.0</Version>
		<Configurations>Debug;R20;R31</Configurations>
		<DefineConstants>NITISCORE20_OR_MORE;NITISCORE</DefineConstants>
		<PackageIconUrl>https://github.com/NickName73/NiTiSCore/blob/main/iconLogoHD.png</PackageIconUrl>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<RepositoryType>git</RepositoryType>
		<RepositoryUrl>https://github.com/NickName73/NiTiSCore</RepositoryUrl>
		<Copyright>Copyright (c) 2021 NickName73</Copyright>
		<SignAssembly>False</SignAssembly>
		<AssemblyOriginatorKeyFile></AssemblyOriginatorKeyFile>
		<PackageProjectUrl>https://www.nuget.org/packages/NiTiSCore</PackageProjectUrl>
		<PackageIcon>iconLogo.png</PackageIcon>
		<PackageTags>NiTiS;NickName73</PackageTags>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)' == 'R31'">
		<TargetFramework>netcoreapp3.1</TargetFramework>
		<DefineConstants>NITISCORE31_OR_MORE;NITISCORE</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='R20|AnyCPU'">
	  <Optimize>True</Optimize>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='R31|AnyCPU'">
	  <Optimize>True</Optimize>
	</PropertyGroup>
	<ItemGroup>
	  <Content Include="iconLogo.png">
	    <CopyToOutputDirectory>Never</CopyToOutputDirectory>
	  </Content>
	  <None Include="LICENSE" Pack="true" PackagePath="" />
	</ItemGroup>
	<ItemGroup>
	  <None Update="iconLogo.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>
</Project>