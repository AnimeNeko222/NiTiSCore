<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<PackageId>NiTiSCore</PackageId>
		<TargetFrameworks>netcoreapp2.0</TargetFrameworks>
		<LangVersion>7.3</LangVersion>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<AssemblyName>NiTiSCore</AssemblyName>
		<AssemblyVersion>1.3.2.0</AssemblyVersion>
		<Authors>NickName73,NiTiS</Authors>
		<Company>NiTiS</Company>
		<Product>NiTiSCore</Product>
		<Title>NiTiSCore</Title>
		<Description>Core for NiTiS projects</Description>
		<Version>1.3.2</Version>
		<Summary>Core for NiTiS projects</Summary>
		<Configurations>Debug;R20;R31</Configurations>
		<DefineConstants>NITISCORE20_OR_MORE;NITISCORE</DefineConstants>
		<PackageIconUrl>https://github.com/NickName73/NiTiSCore/blob/main/iconLogo.png?raw=true</PackageIconUrl>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<RepositoryType>git</RepositoryType>
		<RepositoryUrl>https://github.com/NickName73/NiTiSCore</RepositoryUrl>
		<Copyright>Copyright (c) 2021 NickName73</Copyright>
		<SignAssembly>False</SignAssembly>
		<AssemblyOriginatorKeyFile></AssemblyOriginatorKeyFile>
		<PackageProjectUrl>https://www.nuget.org/packages/NiTiSCore</PackageProjectUrl>
		<PackageIcon>iconLogo.png</PackageIcon>
		<PackageTags>NiTiS;NickName73</PackageTags>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<ImplicitUsings>disable</ImplicitUsings>
		<RootNamespace>NiTiS.Core</RootNamespace>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)' == 'R31'">
		<TargetFrameworks>netcoreapp2.0;netcoreapp3.1</TargetFrameworks>
		<LangVersion>8.0</LangVersion>
		<DefineConstants>NITISCORE31_OR_MORE;NITISCORE</DefineConstants>
		<Optimize>true</Optimize>
	</PropertyGroup>
	<ItemGroup>
	  <None Include="LICENSE" Pack="true" PackagePath="" />
		<None Include="iconLogo.png"></None>
	</ItemGroup>
	<ItemGroup>
	  <None Update="iconLogo.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>
</Project>